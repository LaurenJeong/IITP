          <Column id="FROM" type="DATE" size="256"/>
          <Column id="TO" type="DATE" size="256"/>
          <Column id="DEPT_NM" type="STRING" size="256"/>
          <Column id="PERCENT" type="FLOAT" size="256"/>
        </ColumnInfo>
