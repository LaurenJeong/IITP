          <Objects>
            <Dataset id="viewdataset">
              <ColumnInfo>
                <Column id="ROOM_NM" type="STRING" size="80" description="회의실명"/>
                <Column id="FROM" type="DATE" size="80" description="시작시간"/>
                <Column id="TO" type="DATE" size="80" description="종료시간"/>
                <Column id="DEPT_NM" type="STRING" size="80" description="부서명"/>
                <Column id="PERCENT" type="FLOAT" size="80" description="실행율"/>
              </ColumnInfo>
              <Rows/>
            </Dataset>
          </Objects>
          <Bind>
            <BindItem id="bind_cmbCompany" compid="cmbCompany" propid="value" datasetid="viewdataset" columnid="Company"/>
            <BindItem id="bind_cmbDepartment" compid="cmbDepartment" propid="value" datasetid="viewdataset" columnid="Department"/>
            <BindItem id="bind_edtName" compid="edtName" propid="value" datasetid="viewdataset" columnid="Name"/>
            <BindItem id="bind_edtPhone" compid="edtPhone" propid="value" datasetid="viewdataset" columnid="Phone"/>
            <BindItem id="bind_edtEmail" compid="edtEmail" propid="value" datasetid="viewdataset" columnid="Email"/>
          </Bind>
          <ModelInfo modelserviceid="model" serviceid="" modelid="timelinemodel" filepath="nexacrochart\chartModelService.xmodel">
            <Fields>
              <Field id="ROOM_NM" description="" displaytype="category"/>
              <Field id="FROM" description="" displaytype="seriesfrom"/>
              <Field id="TO" description="" displaytype="seriesto"/>
              <Field id="DEPT_NM" description="" displaytype="none"/>
              <Field id="PERCENT" description="" displaytype="none"/>
            </Fields>
            <Components>
              <Component id="chart" fields="ROOM_NM,FROM,TO,DEPT_NM,PERCENT"/>
            </Components>
          </ModelInfo>
          <ViewTemplateInfo filepath="Chart\TimelineChartViewTemplate.xviewgen">
            <Attribute id="enableanimation" value="true"/>
            <Attribute id="titlealign" value="topcenter"/>
            <Attribute id="itemtextposition" value="none"/>
            <Attribute id="viewpadding" value="20"/>
          </ViewTemplateInfo>
        </View>
        <Static id="staViewAttribute" taborder="3" text="View Attribute" left="0" top="viewSample:10" height="30" cssclass="sta_WF_title02" right="10"/>
        <Grid id="grdViewAttribute" taborder="4" left="0" top="staViewAttribute:0" height="281" right="10" binddataset="dsViewAttribute" autofittype="col" autosizingtype="row" extendsizetype="row">
