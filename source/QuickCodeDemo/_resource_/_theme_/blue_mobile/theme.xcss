.HScrollIndicatorControl
{
	-nexa-padding 		: 0px 10px 5px 10px;
	background 		: transparent;
}
.HScrollIndicatorControl .trackbar
{
	background 		: #E5E7EC;
	border-radius 		: 5px;
}
/* Grid */
.Grid
{
	background : #FFFFFF;
	-nexa-border : 2px solid #0F61FD, 1px solid #D4D4D4, 1px solid #D4D4D4, 1px solid #D4D4D4;
}
/* Grid/head */
.Grid .head
{
	background : #EEEFF3;
	font : bold 18px "Helvetica","Malgun Gothic","Roboto","Apple SD Gothic Neo","Microsoft NeoGothic","Droid sans","sans-serif";
	color : #4A4F5E;
}
.Grid .head .row .cell
{
	-nexa-border : 0px none, 1px solid #D4D4D4, 1px solid #D4D4D4, 0px none;
	-nexa-text-align : center;
}
/* Grid/body */
.Grid .body .row .cell
{
	-nexa-border : 0px none, 1px solid #D4D4D4, 1px solid #D4D4D4, 0px none;
	-nexa-padding : 6px;
}
.Grid .body .row .cell[userstatus=selected]
{
	background : #E0EDFE;
}
/* Grid/summary */
.Grid .summary
{
	background : #4A4F5E;
	font : bold 18px "Helvetica","Malgun Gothic","Roboto","Apple SD Gothic Neo","Microsoft NeoGothic","Droid sans","sans-serif";
	color : #FFFFFF;
}
.Grid .summary .row .cell
{
	-nexa-border : 0px none,1px solid #DBDBDB,0px none,0px none;
	-nexa-padding : 0px 40px 0px 0px;
}
.GridCellControl .cellbutton,.GridCellControl .cellbutton[userstatus=pushed],
.GridCellControl .cellbutton[status=focused],.GridCellControl .cellbutton[userstatus=selected]
{
	-nexa-border : 1px solid #0F61FD;
	-nexa-padding : 0px 15px 0px 15px;
	border-radius : 3px;
	background :#FFFFFF;
	font : bold 18px "Helvetica","Malgun Gothic","Roboto","Apple SD Gothic Neo","Microsoft NeoGothic","Droid sans","sans-serif";
	color : #0F61FD;
	cursor : pointer;
}
.GridCellControl .cellbutton[status=disabled]
{
	-nexa-border : 1px solid #D8D8D8;
	color : #D8D8D8;
	background : #F7F9FA;
	cursor : default;
}
/* GridCellControl */
.GridCellControl .cellexpandbutton,
.GridCellControl .cellexpandbutton[status=focused],
.GridCellControl .cellexpandbutton[userstatus=pushed]
{
	-nexa-icon : url("theme://images/btn_GridExpand_N.png");
	cursor : pointer;
}
.GridCellControl .celltreeitem .treeitembutton[userstatus=expand]
{
	-nexa-icon : url("theme://images/img_TreeExpand.png");
}
.GridCellControl .celltreeitem .treeitembutton[userstatus=collapse]
{
	-nexa-icon : url("theme://images/img_TreeCollapse.png");
}
.GridCellControl .celltreeitem .treeitemimage[userstatus=expand]
{
	-nexa-icon : url("theme://images/img_TreeOpen.png");
}
.GridCellControl .celltreeitem .treeitemimage[userstatus=collapse]
{
	-nexa-icon : url("theme://images/img_TreeClose.png");
}
.GridCellControl .celltreeitem .treeitemimage[userstatus=leaf]
{
	-nexa-icon : url("theme://images/img_subtitle.png");
}
.GridCellControl .celltreeline
{
	-nexa-border : 1px dotted #C0C0C0;
}
/* Menu */
.Menu
{
	background : #FFFFFF;
	font : bold 20px "Helvetica","Malgun Gothic","Roboto","Apple SD Gothic Neo","Microsoft NeoGothic","Droid sans","sans-serif";
	color : #696C72;
}
.Menu .menuitem
{
	-nexa-padding : 0px 30px 0px 30px;
}
.Menu .menuitem[userstatus=selected]
{
	color : #0F61FD;
}
.Menu .menuitem[status=disabled]
{
	color : #D8D8D8;
}
.Menu .nextbutton,.Menu .nextbutton[status=focused],.Menu .nextbutton[userstatus=pushed]
{
	-nexa-icon : url("theme://images/btn_next_N.png");
}
.Menu .nextbutton[status=disabled]
{
	-nexa-icon : url("theme://images/btn_next_D.png");
}
.Menu .prevbutton,.Menu .prevbutton[status=focused],.Menu .prevbutton[userstatus=pushed]
{
	-nexa-icon : url("theme://images/btn_prev_N.png");
}
.Menu .prevbutton[status=disabled]
{
	-nexa-icon : url("theme://images/btn_prev_D.png");
}

/* PopupMenu *//* PopupMenuControl */
.PopupMenu,.PopupMenuControl
{
	-nexa-border : 1px solid #D4D4D4;
	border-radius : 3px;
	background : #FFFFFF;
}
.PopupMenu .prevbutton,.PopupMenu .prevbutton[status=focused],
.PopupMenuControl .prevbutton,.PopupMenuControl .prevbutton[status=focused],
.PopupMenu .prevbutton[userstatus=pushed],.PopupMenu .prevbutton[userstatus=selected],
.PopupMenuControl .prevbutton[userstatus=pushed],.PopupMenuControl .prevbutton[userstatus=selected]
{
	-nexa-icon : url("theme://images/btn_Pmenuprev_N.png");
}
.PopupMenu .prevbutton[status=disabled],
.PopupMenuControl .prevbutton[status=disabled]
{
	-nexa-icon : url("theme://images/btn_Pmenuprev_D.png");
}
.PopupMenu .nextbutton,.PopupMenu .nextbutton[status=focused],
.PopupMenuControl .nextbutton,.PopupMenuControl .nextbutton[status=focused],
.PopupMenu .nextbutton[userstatus=pushed],.PopupMenu .nextbutton[userstatus=selected],
.PopupMenuControl .nextbutton[userstatus=pushed],.PopupMenuControl .nextbutton[userstatus=selected]
{
	-nexa-icon : url("theme://images/btn_Pmenunext_N.png");
}

.PopupMenu .nextbutton[status=disabled],
.PopupMenuControl .nextbutton[status=disabled]
{
	-nexa-icon : url("theme://images/btn_Pmenunext_D.png");
}
.PopupMenu .popupmenuitem,
.PopupMenuControl .popupmenuitem
{
	-nexa-padding : 8px 15px 8px 15px;
	font : 16px "Helvetica","Malgun Gothic","Roboto","Apple SD Gothic Neo","Microsoft NeoGothic","Droid sans","sans-serif";
	color : #696C72;
}
.PopupMenu .popupmenuitem[status=focused],.PopupMenuControl .popupmenuitem[status=focused],
.PopupMenu .popupmenuitem[userstatus=selected],.PopupMenuControl .popupmenuitem[userstatus=selected]
{
	background : #E0EDFE;
}
.PopupMenu .popupmenuitem[status=disabled],.PopupMenuControl .popupmenuitem[status=disabled]
{
	color : #D8D8D8;
}
.PopupMenu .popupmenuitem .popupmenuitemcheckbox,
.PopupMenuControl .popupmenuitem .popupmenuitemcheckbox
{
	background : url('theme://images/ico_menu_check.png') no-repeat left center;
}
.PopupMenu .popupmenuitem .popupmenuitemcheckbox[status=disabled],
.PopupMenuControl .popupmenuitem .popupmenuitemcheckbox[status=disabled]
{
	background : url('theme://images/ico_menu_checkD.png') no-repeat left center;
}
.PopupMenu .popupmenuitem .popupmenuitemexpandimage,
.PopupMenuControl .popupmenuitem .popupmenuitemexpandimage
{
	-nexa-icon : url("theme://images/ico_menu.png");
	-nexa-padding : 0px;
	-nexa-vertical-align : middle;
}
.PopupMenu .popupmenuitem .popupmenuitemtext,
.PopupMenuControl .popupmenuitem .popupmenuitemtext
{
	-nexa-padding : 0px 40px 0px 0px;
	color : #171717;
}
.PopupMenu .popupmenuitem .popupmenuitemhotkeytext,
.PopupMenuControl .popupmenuitem .popupmenuitemhotkeytext
{
	color : #A9B4BF;
	font : 16px "Helvetica","Malgun Gothic","Roboto","Apple SD Gothic Neo","Microsoft NeoGothic","Droid sans","sans-serif";
	-nexa-padding : 0px 5px 0px 0px;
}
/* Static */
.Static
{
	-nexa-word-wrap : char;
}
.Static[status=disabled]
{
	color : #D8D8D8;
}
/* ImageViewer */
.ImageViewer
{
	-nexa-border : 1px solid #D4D4D4;
	border-radius : 3px;
}
.ImageViewer[status=disabled]
{
	-nexa-border : 1px solid #EFEFEF;
	border-radius : 3px;
}
/* Sketch */
.Sketch,.SketchControl
{
	-nexa-border : 1px solid #D4D4D4;
	border-radius : 3px;
	background : #FFFFFF;
	color : #2F333E;
}

.Sketch[status=disabled],.SketchControl[status=disabled]
{
	-nexa-border : 1px solid #EFEFEF;
	background : #F9F9F9;
	color : #D8D8D8;
}
/* ListBox */
.ListBox
{
	-nexa-border : 1px solid #D4D4D4;
	border-radius : 3px;
	background : #FFFFFF;
}
.ListBox[status=disabled]
{
	-nexa-border : 1px solid #EFEFEF;
	border-radius : 3px;
	background : #F7F9FA;
}
.ListBox .listboxitem
{
	-nexa-padding : 7px 0px 7px 15px;
}
.ListBox .listboxitem[status=focused],.ListBox .listboxitem[userstatus=selected]
{
	background : #E0EDFE;
}
.ListBox .listboxitem[status=disabled]
{
	color : #D8D8D8;
}
/* WebBrowser *//* WebView */
.WebBrowser,.WebBrowserControl,.WebView,.WebViewControl
{
	/* No style applied */
}]]></XCSS>
