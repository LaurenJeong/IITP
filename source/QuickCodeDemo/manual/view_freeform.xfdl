<?xml version="1.0" encoding="utf-8"?>
<FDL version="2.1">
  <Form id="view_freeform" width="1030" height="130" titletext="FreeFormViewTemplate" onload="form_onload">
    <Layouts>
      <Layout height="130" mobileorientation="landscape" width="1030">
        <View id="viewSample" taborder="1" text="View00" viewdataset="viewdataset" left="10" top="10" height="109" right="10">
          <Layouts>
            <Layout>
              <Div id="divTitle" left="0" top="0" height="30" right="0">
                <Layouts>
                  <Layout>
                    <Static id="staTitle" left="0" top="0" height="30" width="120" text="상세내역" fittocontents="width" cssclass="sta_subtitle"/>
                  </Layout>
                </Layouts>
              </Div>
              <Static id="staCompanyBg" left="0" top="30" width="250" height="40" cssclass="sta_fieldbox"/>
              <Static id="staCompanyLabel" left="0" top="30" width="100" height="40" text="회사명" cssclass="sta_labelbox_essential"/>
              <Combo id="cmbCompany" left="105" top="35" width="140" height="30" innerdataset="innerdatasetCompany" codecolumn="code" datacolumn="value"/>
              <Static id="staDepartmentBg" left="249" top="30" width="250" height="40" cssclass="sta_fieldbox"/>
              <Static id="staDepartmentLabel" left="249" top="30" width="100" height="40" text="부서명" cssclass="sta_labelbox_essential"/>
              <Combo id="cmbDepartment" left="354" top="35" width="140" height="30" innerdataset="innerdatasetDepartment" codecolumn="code" datacolumn="value"/>
              <Static id="staNameBg" left="498" top="30" width="250" height="40" cssclass="sta_fieldbox"/>
              <Static id="staNameLabel" left="498" top="30" width="100" height="40" text="성명" cssclass="sta_labelbox_essential"/>
              <Edit id="edtName" left="603" top="35" width="140" height="30"/>
