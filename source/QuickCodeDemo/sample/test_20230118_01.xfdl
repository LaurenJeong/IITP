this.btnGetSource_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var sUrl = this.dsMenu.getColumn(this.dsMenu.rowposition,"PGM_URL");
	
	this.gfnGetReadFile(sUrl, "dsContents", "getSource");
};

/**
 * @description Transaction CallBack 함수(선택)
*/
this.fnCallback = function(svcID,errorCode,errorMsg)
{
	// 에러 시 화면 처리 내역
	if(errorCode != 0)
	{
		return;
	}
	
	switch(svcID)
	{
		case "getSource":
			var sContents = this.dsContents.getColumn(0,"contents");
	
			this.txaOutPut.set_value(sContents);
			this.fv_formContents = sContents;
			this.fv_formNode = this.fnGetFormContents(sContents);
			
			//this.gfnLog(this.fv_formNode);
			trace("height : " + this.fv_formNode["attributes"]["height"]);
			break;
	}
};

this.fnGetFormContents = function(sContents) 
{
	var domPar, domDoc;
	var referenceNode, formNode;
	var oContents;
	
	if (system.navigatorname == "nexacro")
	{
		domPar = new nexacro.DomParser();
		domDoc = domPar.parseFromString(sContents);
		
		referenceNode = domDoc.firstChild;
		if(!referenceNode)		return;
		
		// 첫번째 Node
		formNode = referenceNode.firstChild;
	}
	else
	{
		this.gfnLog("웹!!");
		
		var nSIdx = sContents.indexOf('<Form');
		var nEIdx = sContents.indexOf('</FDL>');
		
		sContents = sContents.substring(nSIdx , nEIdx -1);
		sContents = '<?xml version="1.0" encoding="UTF-8"?>' + sContents;
		
		this.gfnLog(sContents);

		domPar = new nexacro.DomParser();
		domDoc = domPar.parseFromString(sContents);
		
		//domPar = new DomParser();
		//domDoc = domPar.parseFromString(sContents,"text/xml");
		
		if(!domDoc)		return;
		
		formNode = domDoc;
	}
	
	if (formNode) {
		oContents = this.gfnXmlToJson(formNode);
	}
	
	return oContents;
};
