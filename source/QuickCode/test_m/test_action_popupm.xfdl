<?xml version="1.0" encoding="utf-8"?>
<FDL version="2.1">
  <Form id="test_action_popupm" width="480" height="768" titletext="모바일 팝업 테스트">
    <Layouts>
      <Layout height="768" mobileorientation="portrait" width="480">
        <Button id="btnPopup" taborder="0" text="팝업&#13;&#10;(선택데이터)" left="10" top="50" width="133" height="60"/>
        <TextArea id="txaOutput" taborder="1" left="10" top="120" wordWrap="english" height="230" right="10"/>
        <Button id="btnPopup2" taborder="2" text="팝업&#13;&#10;(전체데이터)" left="btnPopup:10" top="50" width="133" height="60"/>
        <Button id="btnPopup3" taborder="3" text="팝업&#13;&#10;(변경데이터)" left="btnPopup2:10" top="50" width="133" height="60"/>
        <View id="View00" taborder="4" text="View00" viewdataset="viewdataset" left="10" right="10" top="txaOutput:20" bottom="0">
          <Layouts>
            <Layout>
              <Div id="divTitle" left="0" top="0" height="30" right="0" text="">
                <Layouts>
                  <Layout>
                    <Static id="staTitle" left="0" top="0" height="30" width="170" text="팝업결과 데이터" fittocontents="width" cssclass="sta_WF_title02"/>
                  </Layout>
                </Layouts>
              </Div>
              <Grid id="grdList" left="0" top="30" right="0" bottom="0" binddataset="viewdataset" autofittype="col">
                <Formats>
                  <Format id="default">
                    <Columns>
                      <Column size="80" band="body"/>
                      <Column size="80" band="body"/>
                      <Column size="80" band="body"/>
                    </Columns>
                    <Rows>
                      <Row size="52" band="head"/>
                      <Row size="50" band="body"/>
                    </Rows>
                    <Band id="head">
                      <Cell id="cell_company" col="0" row="0" text="Company"/>
                      <Cell id="cell_department" col="1" row="0" text="Department"/>
                      <Cell id="cell_name" col="2" row="0" text="Name"/>
                    </Band>
                    <Band id="body">
                      <Cell id="cell_company" col="0" row="0" text="bind:company" displaytype="normal" edittype="none"/>
                      <Cell id="cell_department" col="1" row="0" text="bind:department" displaytype="normal" edittype="none"/>
                      <Cell id="cell_name" col="2" row="0" text="bind:name" displaytype="normal" edittype="none"/>
                    </Band>
                    <Band id="summary"/>
                  </Format>
                </Formats>
              </Grid>
            </Layout>
          </Layouts>
          <Objects>
            <Dataset id="viewdataset">
              <ColumnInfo>
                <Column id="company" type="STRING" size="80" description="Company"/>
                <Column id="department" type="STRING" size="80" description="Department"/>
                <Column id="name" type="STRING" size="80" description="Name"/>
              </ColumnInfo>
              <Rows/>
            </Dataset>
          </Objects>
          <ModelInfo modelserviceid="model" serviceid="svcQuickCodeTutorial" modelid="tutorial_DetailEmployee" filepath="svcQuickCodeTutorial.xmodel">
            <Fields>
              <Field id="company" required="false" displaytype="normal" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="true"/>
              <Field id="department" required="false" displaytype="normal" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="true"/>
              <Field id="name" required="false" displaytype="normal" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="true"/>
            </Fields>
            <Components>
              <Component id="divTitle" fields="_viewcomp"/>
              <Component id="grdList" fields="company,department,name"/>
            </Components>
          </ModelInfo>
          <ViewTemplateInfo filepath="GridViewTemplate.xviewgen">
            <Attribute id="titletext" value="팝업결과 데이터"/>
            <Attribute id="usetotalcount" value="false"/>
            <Attribute id="headbandsize" value="52"/>
            <Attribute id="bodybandsize" value="50"/>
            <Attribute id="summbandsize" value="-1"/>
          </ViewTemplateInfo>
        </View>
        <Static id="Static00" taborder="5" text="브라우저실행" left="20" top="10" height="40" cssclass="sta_WF_title02" width="128" fittocontents="width"/>
        <CheckBox id="chkOnCanEvent" taborder="6" text="Action 실행여부" left="158" top="10" width="171" height="40" value="true" fittocontents="width"/>
      </Layout>
    </Layouts>
    <Objects>
      <Dataset id="dsRetPop">
        <ColumnInfo>
          <Column id="company" type="STRING" size="80"/>
          <Column id="department" type="STRING" size="80"/>
          <Column id="name" type="STRING" size="80"/>
        </ColumnInfo>
      </Dataset>
      <PopupAction id="PopupAction00" formurl="test_m::test_action_popupmsub.xfdl" popupstyle="modal" popupid="popup" title="선택데이터" popupleft="-1" popuptop="-1" popupwidth="-1" popupheight="-1" onsuccess="PopupAction00_onsuccess" onerror="PopupAction00_onerror" args="{&quot;returntype&quot; : &quot;currow&quot;}" targetview="View00" popupdatatype="replace" canrun="Common_canrun">
        <Contents/>
      </PopupAction>
      <PopupAction id="PopupAction01" formurl="test_m::test_action_popupmsub.xfdl" popupstyle="modeless" popupid="popup" title="전체데이터" popupleft="-1" popuptop="-1" popupwidth="-1" popupheight="-1" onsuccess="PopupAction00_onsuccess" onerror="PopupAction00_onerror" args="{&quot;returntype&quot; : &quot;alldata&quot;}" targetview="View00" popupdatatype="replace" canrun="Common_canrun">
        <Contents/>
      </PopupAction>
      <PopupAction id="PopupAction02" formurl="test_m::test_action_popupmsub.xfdl" popupstyle="modal" popupid="popup" title="변경데이터" popupleft="-1" popuptop="-1" popupwidth="-1" popupheight="-1" onsuccess="PopupAction00_onsuccess" onerror="PopupAction00_onerror" args="{&quot;returntype&quot; : &quot;changedata&quot;}" targetview="View00" popupdatatype="replace" canrun="Common_canrun">
        <Contents/>
      </PopupAction>
    </Objects>
    <Trigger>
      <TriggerItem id="item0" triggertype="Click" triggerview="test_action_popupm" triggerobj="btnPopup" targetaction="PopupAction00" condition=""/>
      <TriggerItem id="item1" triggertype="Click" triggerview="test_action_popupm" triggerobj="btnPopup2" targetaction="PopupAction01" condition=""/>
      <TriggerItem id="item2" triggertype="Click" triggerview="test_action_popupm" triggerobj="btnPopup3" targetaction="PopupAction02" condition=""/>
    </Trigger>
    <Script type="xscript5.1"><![CDATA[this.Common_canrun = function(obj, e:nexacro.ActionRunEventInfo)
{
	this.txaOutput.insertText("\n" + "Common_canrun " + obj.name +" : " + this.chkOnCanEvent.value);
	
	return this.chkOnCanEvent.value;
};

this.PopupAction00_onerror = function(obj:nexacro.PopupAction, e:nexacro.ActionErrorEventInfo)
{
	//trace("PopupAction00_onerror" + e.userdata);
	this.txaOutput.insertText("\n" + "PopupAction00_onerror");
	this.txaOutput.insertText("\n - userdata : " + e.userdata);
};

this.PopupAction00_onsuccess = function(obj:nexacro.PopupAction, e:nexacro.ActionSuccessEventInfo)
{
	//trace("PopupAction00_onsuccess" + e.userdata);
	this.txaOutput.insertText("\n" + "PopupAction00_onsuccess");
	this.txaOutput.insertText("\n - userdata : " + e.userdata);
};]]></Script>
  </Form>
</FDL>
