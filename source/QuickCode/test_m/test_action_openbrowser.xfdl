<?xml version="1.0" encoding="utf-8"?>
<FDL version="2.1">
  <Form id="test_action_openbrowser" width="480" height="650" titletext="브라우저실행">
    <Layouts>
      <Layout height="650" width="480" mobileorientation="portrait">
        <Button id="Button03" taborder="0" left="20" top="67" height="60" text="Open Google" right="20"/>
        <View id="View00" taborder="1" text="View00" viewdataset="viewdataset" left="16" top="Button03:20" width="448" height="80">
          <Layouts>
            <Layout>
              <Static id="stBg" left="0" top="0" right="0" bottom="0" cssclass="sta_searchbox"/>
              <Button id="btnSearch" width="100" height="60" right="10" bottom="10" cssclass="btn_search" text="Search"/>
              <Static id="staAddressLabel" left="10" top="10" width="95" height="60" text="Address" cssclass="sta_label"/>
              <Edit id="edtAddress" left="110" top="10" width="208" height="60"/>
            </Layout>
          </Layouts>
          <Objects>
            <Dataset id="viewdataset">
              <ColumnInfo>
                <Column id="address" type="STRING" size="256" description="Address"/>
              </ColumnInfo>
              <Rows>
                <Row>
                  <Col id="address">https://www.google.com/</Col>
                </Row>
              </Rows>
            </Dataset>
          </Objects>
          <Bind>
            <BindItem id="bind_edtAddress" compid="edtAddress" propid="value" datasetid="viewdataset" columnid="address"/>
          </Bind>
          <ModelInfo modelserviceid="model" serviceid="svcQuickCodeTutorial" modelid="tutorial_DetailEmployee" filepath="svcQuickCodeTutorial.xmodel">
            <Fields>
              <Field id="address" required="false" comptype="edit" fieldwidth="100%" fieldheight="30px" labelwidth="100" cssclass="" labelcssclass="sta_label" usecomp="true"/>
            </Fields>
            <Components>
              <Component id="stBg" fields="_viewcomp"/>
              <Component id="btnSearch" fields="_viewcomp"/>
              <Component id="staAddressLabel" fields="address"/>
              <Component id="edtAddress" fields="address"/>
            </Components>
          </ModelInfo>
          <ViewTemplateInfo filepath="SearchViewTemplate.xviewgen">
            <Attribute id="direction" value="horizontal"/>
            <Attribute id="halign" value="left"/>
            <Attribute id="valign" value="top"/>
            <Attribute id="searchbuttontype" value="bottom"/>
            <Attribute id="searchbuttontext" value="Search"/>
            <Attribute id="searchbuttoncssclass" value="btn_search"/>
            <Attribute id="searchbgcssclass" value="sta_searchbox"/>
          </ViewTemplateInfo>
        </View>
        <Static id="Static00" taborder="2" text="브라우저실행" left="20" top="10" height="40" cssclass="sta_WF_title02" width="128" fittocontents="width"/>
        <CheckBox id="chkOnCanEvent" taborder="3" text="Action 실행여부" left="Static00:10" top="10" width="171" height="40" value="true" fittocontents="width"/>
        <TextArea id="txtOutput" taborder="4" left="20" top="View00:20" right="20" bottom="20"/>
      </Layout>
    </Layouts>
    <Objects>
      <OpenBrowserAction id="OpenBrowserAction00" url="https://www.google.com/" newwindow="true" targetview="test_action_openbrowser" canrun="OpenBrowserAction_canrun" onerror="OpenBrowserAction_onerror" onsuccess="OpenBrowserAction_onsuccess">
        <Contents/>
      </OpenBrowserAction>
      <OpenBrowserAction id="OpenBrowserAction01" url="expr:[address]" newwindow="true" targetview="View00" canrun="OpenBrowserAction_canrun" onerror="OpenBrowserAction_onerror" onsuccess="OpenBrowserAction_onsuccess">
        <Contents/>
      </OpenBrowserAction>
    </Objects>
    <Trigger>
      <TriggerItem id="item0" triggertype="Click" triggerview="test_action_openbrowser" triggerobj="Button03" targetaction="OpenBrowserAction00" condition=""/>
      <TriggerItem id="item1" triggertype="Click" triggerview="View00" triggerobj="btnSearch" targetaction="OpenBrowserAction01" condition=""/>
    </Trigger>
    <Script type="xscript5.1"><![CDATA[
this.OpenBrowserAction_canrun = function(obj:nexacro.OpenBrowserAction, e:nexacro.ActionRunEventInfo)
{
	this.txtOutput.insertText("\n" + "Common_canrun " + obj.name +" : " + this.chkOnCanEvent.value);
	
	return this.chkOnCanEvent.value;
};

this.OpenBrowserAction_onerror = function(obj:nexacro.Action, e:nexacro.ActionErrorEventInfo)
{
	this.txtOutput.insertText("\n" + obj.name + "_onerror : " + e.userdata);
};

this.OpenBrowserAction_onsuccess = function(obj:nexacro.Action, e:nexacro.ActionSuccessEventInfo)
{
	this.txtOutput.insertText("\n" + obj.name + "_onsuccess : " + e.userdata);
};
]]></Script>
  </Form>
</FDL>
