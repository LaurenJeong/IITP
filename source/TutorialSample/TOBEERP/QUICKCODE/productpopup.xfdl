                <Column id="COLOR" type="STRING" size="80" description="색상"/>
                <Column id="UNIT" type="STRING" size="80" description="단위"/>
                <Column id="BARCODE" type="STRING" size="80" description="바코드"/>
                <Column id="COMPANY_CODE" type="STRING" size="80" description="제조회사"/>
                <Column id="RECEIVING_PRICE" type="INT" size="80" description="입고가"/>
                <Column id="WHOLESALE_PRICE" type="INT" size="80" description="도매가"/>
                <Column id="RETAIL_PRICE" type="INT" size="80" description="소매가"/>
                <Column id="SPECIAL_PRICE1" type="INT" size="80" description="특별가1"/>
                <Column id="SPECIAL_PRICE2" type="INT" size="80" description="특별가2"/>
                <Column id="SPECIAL_PRICE3" type="INT" size="80" description="특별가3"/>
                <Column id="TAX_FREE_CHECK" type="STRING" size="80" description="면세여부"/>
                <Column id="DISCOUNT_RATE" type="INT" size="80" description="판매단가 할인할증율"/>
                <Column id="BASIC_STOCK_COUNT" type="INT" size="80" description="기초재고"/>
                <Column id="NOW_STOCK_COUNT" type="INT" size="80" description="재고수량"/>
                <Column id="BOX_COUNT" type="INT" size="80" description="박스수량"/>
                <Column id="UNIT_COUNT" type="INT" size="80" description="박스당 낱개"/>
                <Column id="SAFETY_STOCK_COUNT" type="INT" size="80" description="안전재고"/>
                <Column id="PRODUCT_STORAGE_AREA" type="STRING" size="80" description="품목위치"/>
                <Column id="COUNT_PER_BOX" type="INT" size="80" description="낱개"/>
              </ColumnInfo>
              <Rows/>
            </Dataset>
            <Dataset id="innerdatasetCompanyCode">
              <ColumnInfo>
                <Column id="code" type="STRING" size="256"/>
                <Column id="value" type="STRING" size="256"/>
              </ColumnInfo>
              <Rows/>
            </Dataset>
          </Objects>
          <ModelInfo modelserviceid="model" serviceid="getproductlist" modelid="dsproduct" filepath="UI_TEST\product\getproductlist.xmodel">
            <Fields>
              <Field id="COUNT_PER_BOX" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="PRODUCT_STORAGE_AREA" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="SAFETY_STOCK_COUNT" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="UNIT_COUNT" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="BOX_COUNT" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="NOW_STOCK_COUNT" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="BASIC_STOCK_COUNT" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="DISCOUNT_RATE" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="TAX_FREE_CHECK" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="SPECIAL_PRICE3" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="SPECIAL_PRICE2" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="SPECIAL_PRICE1" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="RETAIL_PRICE" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="WHOLESALE_PRICE" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="RECEIVING_PRICE" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="BARCODE" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="UNIT" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="COLOR" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="STANDARD" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="ASSET_GROUPS" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="PRODUCT_GROUPS" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="PRODUCT_CODE" displaytype="auto" edittype="none" fieldwidth="80" cssclass="" summprop="none" usecomp="false"/>
              <Field id="PRODUCT_NAME" displaytype="auto" edittype="none" fieldwidth="260" cssclass="" summprop="none" usecomp="true"/>
              <Field id="COMPANY_CODE" label="제조회사" displaytype="combotext" edittype="none" fieldwidth="200" cssclass="" summprop="none" usecomp="true"/>
            </Fields>
            <Components>
              <Component id="grdList" fields="PRODUCT_NAME,COMPANY_CODE"/>
            </Components>
          </ModelInfo>
          <ViewTemplateInfo filepath="Asset\GridViewTemplate.xviewgen">
            <Attribute id="usetotalcount" value="false"/>
            <Attribute id="headbandsize" value="38"/>
            <Attribute id="bodybandsize" value="38"/>
            <Attribute id="summbandsize" value="-1"/>
          </ViewTemplateInfo>
        </View>
        <Static id="staMiddlebar" taborder="2" text="품목 찾기(QuickCode)" left="20" top="20" height="28" cssclass="sta_WF_title02" right="140" onclick="divManufactCo_staMiddlebar_onclick"/>
        <Button id="btnClose" taborder="3" top="20" height="28" cssclass="btn_WF_popup_close" onclick="btnClose_onclick" right="20" width="28"/>
      </Layout>
    </Layouts>
    <Objects>
      <SearchDBAction id="SearchDBAction00" servicemodel="getproductlist" serviceurl="IP::getproductcode.jsp" outputdatasets="viewList.form.innerdatasetCompanyCode=dsmanufacture">
        <Contents/>
      </SearchDBAction>
      <SearchDBAction id="SearchDBAction01" servicemodel="getmainlist" serviceurl="IP::getproductlist.jsp" inputdatasets="dsinput=viewSearch.form.viewdataset " outputdatasets="viewList.form.viewdataset=dsproduct" onsuccess="SearchDBAction01_onsuccess">
        <Contents/>
      </SearchDBAction>
      <PopupCloseAction id="PopupCloseAction00" targetview="productpopup" returntype="none">
        <Contents/>
      </PopupCloseAction>
      <PopupCloseAction id="PopupCloseAction01" targetview="viewList" returntype="currow">
        <Contents/>
      </PopupCloseAction>
    </Objects>
    <Trigger>
      <TriggerItem id="item0" triggertype="Form Init" triggerview="productpopup" triggerobj="productpopup" targetaction="SearchDBAction00" condition=""/>
      <TriggerItem id="item1" triggertype="Click" triggerview="viewSearch" triggerobj="btnSearch" targetaction="SearchDBAction01" condition=""/>
      <TriggerItem id="item2" triggertype="Click" triggerview="productpopup" triggerobj="btnClose" targetaction="PopupCloseAction00" condition=""/>
      <TriggerItem id="item3" triggertype="Click" triggerview="viewList" triggerobj="grdList" targetaction="PopupCloseAction01" condition=""/>
    </Trigger>
    <Script type="xscript5.1"><![CDATA[/**
 * @description edtSearch onkeydown event : 품목 조회
 */
this.viewSearch_edtProduct_onkeydown = function(obj:nexacro.Edit,e:nexacro.KeyEventInfo)
{
	if(e.keycode == 13)
	{
		this.SearchDBAction01.run();
	}
};

/**
 * @description Transaction CallBack 함수
 */
this.SearchDBAction01_onsuccess = function(obj:nexacro.Action, e:nexacro.TranActionSuccessEventInfo)
{
	this.viewList.form.grdList.setFocus();
};

/**
 * @description grdProductList oncellclick event : 품목 선택
 */
this.viewList_grdList_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	this.PopupCloseAction01.run();
};

/**
 * @description grdProductList onkeydown event : 품목 선택
 */
this.viewList_grdList_onkeydown = function(obj:nexacro.Grid,e:nexacro.KeyEventInfo)
{
	if(e.keycode == 13)
	{
		this.PopupCloseAction01.run();
	}
};



]]></Script>
