<?xml version="1.0" encoding="utf-8"?>
<FDL version="2.1">
  <Form id="productpopup" width="520" height="330" titletext="품목찾기">
    <Layouts>
      <Layout height="330" mobileorientation="landscape" width="520">
        <View id="viewSearch" taborder="0" text="viewSearch" viewdataset="viewdataset" left="20" top="55" height="40" right="20">
          <Layouts>
            <Layout>
              <Static id="stBg" left="0" top="0" right="0" bottom="0" cssclass="sta_cm_box01L" taborder="3"/>
              <Button id="btnSearch" width="70" height="28" right="5" bottom="6" cssclass="btn_WF_add01" text="조회" taborder="2"/>
              <Static id="staProductLabel" left="5" top="6" width="95" height="28" text="품목명" cssclass="sta_WF_label" taborder="0"/>
              <Edit id="edtProduct" left="105" top="6" width="280" height="28" onkeydown="viewSearch_edtProduct_onkeydown" taborder="1"/>
            </Layout>
          </Layouts>
          <Objects>
            <Dataset id="viewdataset">
              <ColumnInfo>
                <Column id="PRODUCT" type="STRING" size="80" description="품목명"/>
              </ColumnInfo>
              <Rows>
                <Row/>
              </Rows>
            </Dataset>
          </Objects>
          <Bind>
            <BindItem id="bind_edtProduct" compid="edtProduct" propid="value" datasetid="viewdataset" columnid="PRODUCT"/>
          </Bind>
          <ModelInfo modelserviceid="model" serviceid="getproductlist" modelid="dsinput" filepath="UI_TEST\product\getproductlist.xmodel">
            <Fields>
              <Field id="PRODUCT" description="" comptype="auto" compwidth="100%" compheight="28px" labelsize="100" cssclass="" labelcssclass="sta_WF_label" usecomp="true"/>
            </Fields>
            <Components>
              <Component id="stBg" fields="_viewcomp"/>
